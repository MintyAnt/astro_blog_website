{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "func-names": 0,
        "one-var": 0,
        "strict": 1,
        "indent": [2,4, {"SwitchCase": 1}],
        "id-length": 0,
        "spaced-comment": 0,
        "no-unused-expressions": 0,
        "react/jsx-indent": 0,
        "react/jsx-filename-extension": 0
    }
}
